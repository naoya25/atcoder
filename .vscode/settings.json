{
  "cSpell.words": [
    "collatz",
    "deque",
    "Fenwick",
    "itertools",
    "kdir",
    "KHTML",
    "ntight",
    "popleft",
    "setrecursionlimit",
    "sortedcontainers",
    "ssum",
    "submasks",
    "takahashi",
    "trae"
  ],
  "[python]": {
    "editor.formatOnType": true,
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "pylint.args": [
    "\"pylint.args\": [\"--disable=C0111\", \"--max-line-length=120\"]"
  ],
  "code-runner.clearPreviousOutput": true,
  "code-runner.executorMap": {
    "dart": "dart run $dir/$fileName < ./input.txt",
    "python": "python3 -u $dir/$fileName < ./input.txt"
  }
}
